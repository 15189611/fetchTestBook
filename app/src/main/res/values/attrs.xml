<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="common_SimpleViewBehavior">
        <attr name="common_svb_dependOn" format="reference"/>
        <attr name="common_svb_dependType">
            <enum name="height" value="0"/>
            <enum name="width" value="1"/>
            <enum name="x" value="2"/>
            <enum name="y" value="3"/>
        </attr>
        <attr name="common_svb_dependTargetX" format="reference|dimension"/>
        <attr name="common_svb_dependTargetY" format="reference|dimension"/>
        <attr name="common_svb_dependTargetWidth" format="reference|dimension"/>
        <attr name="common_svb_dependTargetHeight" format="reference|dimension"/>
        <attr name="common_svb_targetX" format="reference|dimension"/>
        <attr name="common_svb_targetY" format="reference|dimension"/>
        <attr name="common_svb_targetWidth" format="reference|dimension"/>
        <attr name="common_svb_targetHeight" format="reference|dimension"/>
        <attr name="common_svb_targetBackgroundColor" format="reference|color"/>
        <attr name="common_svb_targetAlpha" format="float"/>
        <attr name="common_svb_targetRotateX" format="float"/>
        <attr name="common_svb_targetRotateY" format="float"/>
        <attr name="common_svb_animation" format="reference"/>
    </declare-styleable>
</resources>